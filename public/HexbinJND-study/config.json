{
  "$schema": "https://raw.githubusercontent.com/reVISit-studies/study/main/src/parser/StudyConfigSchema.json",
  "studyMetadata": {
    "title": "Hexbin Plot JND Study",
    "version": "pilot",
    "authors": ["The reVISit Team"],
    "date": "2024-12-26",
    "description": "This is a reVISit variation study of JND(Just Noticeable Difference) Hexbin Plot experiment.",
    "organizations": ["University of Utah", "WPI", "University of Toronto"]
  },
  "uiConfig": {
    "contactEmail": "contact@revisit.dev",
    "helpTextPath": "HexbinJND-study/assets/help.md",
    "logoPath": "revisitAssets/revisitLogoSquare.svg",
    "withProgressBar": false,
    "autoDownloadStudy": false,
    "sidebar": false,
    "urlParticipantIdParam": "PROLIFIC_PID",
    "studyEndMsg": "**Thank you for completing the study. You may click this link and return to Prolific**: [yourProlificLink](yourProlificLink)"
  },
  "baseComponents": {
    "experimentRevised": {
      "type": "react-component",
      "path": "jnd-replication/vistaJND/src/components/vis/JNDHexbin.tsx",
      "response": [{ 
        "id": "hexbinSelections",
        "prompt": "",
        "required": true,
        "location": "aboveStimulus",
        "type": "numerical",
        "hidden": true
      }
      ]
    },
    "practiceBase": {
      "type": "react-component",
      "path": "jnd-replication/vistaJND/src/components/vis/PracticeHexbin.tsx",
      "nextButtonLocation": "belowStimulus",
      "response": []
    }
  },
  "components": {
    "introduction": {
      "type": "markdown",
      "path": "HexbinJND-study/assets/introduction.md",
      "response": [
        {
            "id": "prolificId",
            "prompt": "Please enter your Prolific ID",
            "required": true,
            "location": "belowStimulus",
            "type": "shortText",
            "placeholder": "Prolific ID",
            "paramCapture": "PROLIFIC_PID"
        }
      ]
    },
    "consent": {
      "type": "markdown",
      "path": "ScatterJND-study/assets/consent.md",
      "nextButtonText": "Agree",
      "response": [
        {
          "id": "signature",
          "prompt": "Your signature",
          "location": "belowStimulus",
          "type": "shortText",
          "placeholder": "Please provide your signature"
        },
        {
          "id": "accept",
          "prompt": "Do you consent to the study and wish to continue?",
          "requiredValue": "Accept",
          "location": "belowStimulus",
          "type": "radio",
          "options": [
            "Decline",
            "Accept"
          ]
        }
      ]
    },
    "training": {
      "type": "markdown",
      "path": "HexbinJND-study/assets/training.md",
      "response": []
    },
    "practice1": {
      "baseComponent": "practiceBase",
      "parameters": { "r1": 0.3, "r2": 0.7 }
    },
    "practice2": {
      "baseComponent": "practiceBase",
      "parameters": { "r1": 0.9, "r2": 0.6 }
    },
    "practice3": {
      "baseComponent": "practiceBase",
      "parameters": { "r1": 0.6, "r2": 0.3 }
    },
    "practice4": {
      "baseComponent": "practiceBase",
      "parameters": { "r1": 0.6, "r2": 0.9 }
    },
    "practice5": {
      "baseComponent": "practiceBase",
      "parameters": { "r1": 0.3, "r2": 0.1 }
    },
    "practice6": {
      "baseComponent": "practiceBase",
      "parameters": { "r1": 0.5, "r2": 0.3 }
    },
    "practice7": {
      "baseComponent": "practiceBase",
      "parameters": { "r1": 0.9, "r2": 0.8 }
    },
    "practice8": {
      "baseComponent": "practiceBase",
      "parameters": { "r1": 0.6, "r2": 0.7 }
    },
    "practice9": {
      "baseComponent": "practiceBase",
      "parameters": { "r1": 0.99, "r2": 0.9 }
    },
    "begin": {
      "type": "markdown",
      "path": "ParallelJND-study/assets/begin.md",
      "response": []
    },
    "trial": {
      "baseComponent": "experimentRevised"
    }
  },
  "sequence": {
    "order": "fixed",
    "components": [
      "introduction",
      "consent",
      "training",
      {
        "order": "fixed",
        "components": [
          "practice1", "practice2", "practice3",
          "practice4", "practice5", "practice6",
          "practice7", "practice8", "practice9"
        ]
      },
      "begin",
      {
        "order": "fixed",
        "components": [
          {
            "order": "dynamic",
            "id": "below3",
            "functionPath": "jnd-replication/vistaJND/src/components/vis/JNDDynamic.tsx",
            "parameters": {
              "r1": 0.3,
              "r2": 0.1,
              "above": false,
              "name": "below3",
              "index": 13
            }
          },{
            "order": "dynamic",
            "id": "above3",
            "functionPath": "jnd-replication/vistaJND/src/components/vis/JNDDynamic.tsx",
            "parameters": {
              "r1": 0.3,
              "r2": 0.5,
              "above": true,
              "name": "above3",
              "index": 14
            }
          },
          {
            "order": "dynamic",
            "id": "below6",
            "functionPath": "jnd-replication/vistaJND/src/components/vis/JNDDynamic.tsx",
            "parameters": {
              "r1": 0.6,
              "r2": 0.5,
              "above": false,
              "name": "below6",
              "index": 15
            }
          },
          {
            "order": "dynamic",
            "id": "above6",
            "functionPath": "jnd-replication/vistaJND/src/components/vis/JNDDynamic.tsx",
            "parameters": {
              "r1": 0.6,
              "r2": 0.7,
              "above": true,
              "name": "above6",
              "index": 16
            }
          },
          {
            "order": "dynamic",
            "id": "below9",
            "functionPath": "jnd-replication/vistaJND/src/components/vis/JNDDynamic.tsx",
            "parameters": {
              "r1": 0.9,
              "r2": 0.8,
              "above": false,
              "name": "below9",
              "index": 17
            }
          },
          {
            "order": "dynamic",
            "id": "above9",
            "functionPath": "jnd-replication/vistaJND/src/components/vis/JNDDynamic.tsx",
            "parameters": {
              "r1": 0.9,
              "r2": 0.99,
              "above": true,
              "name": "above9",
              "index": 18
            }
          }
        ]
      }
    ]
  }
}